.moveTo(0, yValue);\n          ctx.lineTo(canvas.width, yValue);\n          ctx.strokeStyle = style;\n          ctx.stroke();\n        }\n\n        if (line.text) {\n          ctx.fillStyle = style;\n          ctx.fillText(line.text, 0, yValue + ctx.lineWidth);\n        }\n      }\n      return;\n    }\n  },\n};\nimport { HorizontalBar } from \"vue-chartjs\";\nexport default {\n  extends: HorizontalBar,\n  beforeMount() {\n    this.addPlugin(horizonalLinePlugin);\n  },\n  mounted() {\n    this.renderChart(\n      {\n        labels: [\"Data\"],\n        \n        datasets: [\n          {\n            \n            barThickness: 20,\n            barPercentage: 1,\n            // label: \"Data Sync\",\n            backgroundColor: \"#f87979\",\n            data: [40],\n          },\n          {\n            barThickness: 20,\n            barPercentage: 1,\n            // label: \"PDF Generation\",\n            backgroundColor: \"#3D5B96\",\n            data: [50],\n          },\n          {\n            barThickness: 20,\n            barPercentage: 1,\n            // label: \"Email Sender\",\n            backgroundColor: \"#1EFFFF\",\n            data: [20],\n            \n          },\n        ],\n        \n      },\n      {\n        // responsive: true,\n        // maintainAspectRatio: false,\n        plugins: {\n            legend: false // Hide legend\n        },\n        scales: {\n          xAxes: [\n            {\n              stacked: true,\n              categoryPercentage: 0.5,\n              barPercentage: 1,\n            },\n          ],\n          yAxes: [\n            {\n              stacked: true,\n            },\n          ],\n        },\n      }\n    );\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}