,CAAA","sourcesContent":["<template>\n    <div>\n        <form @submit=\"submitData\">\n        <label>User Form</label><br><br> \n        <label v-if=\"error.name\" class=\"name\">{{error.name}}<br></label>\n        <input type=\"text\" placeholder=\"Enter the name\" v-model=\"users.name\"/><br><br>\n         <label v-if=\"error.email\" class=\"name\">{{error.email}}<br></label>\n        <input type=\"text\" placeholder=\"Enter the email\" v-model=\"users.email\"/><br><br>\n       \n        <button  type=\"submit\">Submit</button>\n        </form>\n    </div>\n</template>\n<script>\nimport axios from 'axios'\nexport default {\n\n    name:\"FormValidation\",\n    data(){\n       return{\n            users:{\n                name:null,\n                email:null\n            },\n            error:{\n                name:null,\n                email:null\n            }\n       }\n    },\n    methods:{\n        submitData(e){\n            if(this.users.name && this.users.email){\n                console.warn(this.users)\n            }\n            if(!this.users.name){\n               this.error.name=\"*Name is Required\"\n            }\n            else{\n                this.error.name=null;\n            }\n            if(!this.users.email){\n                this.error.email=\"*Email is Required\"\n            }\n            else{\n                this.error.email=null;\n            }\n            console.warn(this.error);\n            alert(JSON.stringify(this.users));\n            e.preventDefault();\n            \n        }\n    },\n    mounted () {\n    axios\n      .post('http://localhost:8888/person',JSON.stringify(this.users))\n      .then(response => (console.warn(response)))\n  }\n}\n</script>\n<style>\n.name{\n    color: red;\n}\n</style>"],"sourceRoot":"src/components"}]}